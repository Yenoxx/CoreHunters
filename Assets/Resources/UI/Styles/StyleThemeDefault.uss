* {
    --color-border: rgb(255, 255, 255);
    --color-main: rgb(48, 48, 48);
    --color-main-pressed: rgb(64, 64, 64);
    --color-side: rgb(96, 96, 96);
    --color-dimmed: rgba(0, 0, 0, 0.4);
    --color-dimmed-half: rgba(0, 0, 0, 0.2);
